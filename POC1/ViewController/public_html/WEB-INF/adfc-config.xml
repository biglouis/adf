<?xml version="1.0" encoding="UTF-8" ?>
<adfc-config xmlns="http://xmlns.oracle.com/adf/controller" version="1.2">
  <view id="Welcome">
    <page>/pages/Welcome.jsf</page>
  </view>
  <task-flow-call id="Taskflow1">
    <task-flow-reference>
      <document>/WEB-INF/taskflows/boundedTaskFlow1.xml</document>
      <id>boundedTaskFlow1</id>
    </task-flow-reference>
  </task-flow-call>
  <task-flow-call id="Taskflow2">
    <task-flow-reference>
      <document>/WEB-INF/taskflows/boundedTaskFlow2.xml</document>
      <id>boundedTaskFlow2</id>
    </task-flow-reference>
  </task-flow-call>
  <control-flow-rule id="__1">
    <from-activity-id>Welcome</from-activity-id>
    <control-flow-case id="__2">
      <from-outcome>callTaskflow1</from-outcome>
      <to-activity-id>Taskflow1</to-activity-id>
    </control-flow-case>
    <control-flow-case id="__3">
      <from-outcome>callTaskflow2</from-outcome>
      <to-activity-id>Taskflow2</to-activity-id>
    </control-flow-case>
  </control-flow-rule>
</adfc-config>
