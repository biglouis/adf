  <task-flow-definition id="boundedTaskFlow1">
    <default-activity>taskFlow1view</default-activity>
    <data-control-scope>
      <shared/>
    </data-control-scope>
    <view id="taskFlow1view">
      <page>/taskFlow1view.jsf</page>
    </view>
    <task-flow-return id="taskFlowReturn">
      <outcome>
        <name>taskFlowReturn</name>
      </outcome>
    </task-flow-return>
    <control-flow-rule id="__1">
      <from-activity-id>taskFlow1view</from-activity-id>
      <control-flow-case id="__2">
